<%= form_for @attraction do |f| %>
  Name: <%= f.text_field :name %><br>
  Nausea Rating (1 to 5): <%= f.text_field :nausea_rating %><br>
  Happiness Rating (1 to 5): <%= f.text_field :happiness_rating %><br>
  Tickets (1 to 5): <%= f.text_field :tickets %><br>
  Minimum Height Requirement: <%= f.text_field :min_height %><br>
  <%= f.submit %>
<% end %>